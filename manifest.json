{
  "manifest_version": 2,
  "name": "Domain Watcher",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/green.png",
      "32": "icons/green.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/green.png",
    "32": "icons/green.png"
  }
}
